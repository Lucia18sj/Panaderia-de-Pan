<header>
    <a href="/"><img src="https://res.cloudinary.com/dexasecyo/image/upload/v1731965711/logo_zremz9.png" style="width: 65%; height: 15%;">
    </a>

    <nav class="navbar">
        <a href="/">Home</a>
        <a href="/us">Nosotros</a>
        <a href="/contact">Contacto</a>
    </nav>
    <div class="icons" style="display: flex; align-items: center; gap: 10px;">
        <% if (name === 'Admin') { %>
            <a href="/api/administrator" class="fa-solid fa-user-shield"></a>
            <span style="font-size: 16px;">Hola, <%= name %></span>
        <% } else if (name) { %>
            <a href="/login" class="fa-solid fa-user"></a>
            <span style="font-size: 16px;">Hola, <%= name %></span>
        <% } else { %>
            <a href="#" class="fa-solid fa-user-minus"></a>
        <% } %>
        <a href="#"><div class="fas fa-search"></div></a>
        <a href="<%= customerId ? `/api/cart/${customerId}` : '/login' %>"><div class="fas fa-shopping-cart"></div></a>
    </div>
</header>
