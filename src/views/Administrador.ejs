<body style="background-color: #123D44">
    <%- include('partials/navBar', { name: name, customerId: customerId, lastname: lastname}) %>
    <link rel="stylesheet" href="/css/admin.css">
    <h1>Administrador</h1>
    <div class="cart-container mt-2 mb-5" style="width:60%;">
        <div class="cart-items">
            <a href="/api/administrator/inventario" class="cart-item mb-4">
                <div class="icon">
                    <i class="fa-solid fa-boxes-stacked"></i>                </div>
